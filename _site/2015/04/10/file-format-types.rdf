  <rdf:Description rdf:about="http://pcdm.org/file-format-types#">
    <dcterms:title xml:lang="en">PCDM File Format Genres</dcterms:title>
    <rdfs:comment xml:lang="en">PCDM File Format Genre ontology.  Provides terms for describing file formats
      selected from vocabularies such as DCMIType, UDFRS, Nepomuk, Pronom, and MARC Resource
      Types.</rdfs:comment>
    <dcterms:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2015-10-14</dcterms:modified>
    <dcterms:publisher rdf:resource="http://www.duraspace.org/"/>
    <rdfs:seeAlso rdf:resource="https://github.com/duraspace/pcdm/wiki"/>
    <owl:versionInfo>2015/10/14</owl:versionInfo>
  </rdf:Description>
